    "h2o2": "5.1.0",
    "hapi": "13.2.2",
    "hapi-auth-basic": "4.1.0",
    "hapi-auth-hawk": "3.0.1",
    "hapi-bookshelf-models": "1.7.1",
    "hapi-redis": "6.0.0",
    "hapi-relations": "1.0.0",
    "hapi-swagger": "4.3.1",
    "inert": "3.2.0",
