    "relations": "0.5.0",
    "uuid": "2.0.1",
    "vision": "4.0.1"
  },
  "devDependencies": {
    "async": "2.0.0-rc.2",
    "coins-validate": "2.0.6",
    "dom-storage": "2.0.2",
    "fakeredis": "1.0.2",
    "fs-extra": "0.26.7",
    "getport": "0.1.0",
    "gh-pages": "0.11.0",
    "hawk": "4.1.2",
    "inject-then": "2.0.5",
    "istanbul": "0.4.2",
    "jscs": "2.11.0",
    "jsdoc": "3.4.0",
    "jshint": "2.9.1",
    "json-loader": "0.5.4",
    "memdown": "1.1.2",
    "mocha": "2.4.5",
    "node-localstorage": "1.1.2",
    "pg": "4.5.2",
    "pouchdb": "5.3.1",
    "rename-keys": "1.1.2",
    "should": "8.3.0",
    "webpack": "1.12.14"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "pretest": "npm run build",
    "postinstall": "npm run build",
    "test": "mocha --timeout=10000 -R spec 'src/test/integration/*.js' 'src/test/unit/*.js'",
    "start": "node src/index.js",
    "lint": "jscs --fix src/",
    "validate": "npm ls",
    "update-codegen": "node scripts/update-codegen.js",
    "generate-client": "node scripts/generate-client.js",
    "postgenerate-client": "webpack",
    "lintfix": "jscs --fix src/**/*.js test/**/*.js",
    "postlint": "jshint src/**/*.js test/**/*.js",
    "preversion": "git checkout master && git pull && npm ls",
    "docs": "jsdoc -d docs -r -R README.md src/ && git add docs/*",
    "deploy-docs": "gh-pages -d docs",
    "postpublish": "npm run deploy-docs",
    "publish-patch": "npm run preversion && npm version patch && git push origin master --tags && npm publish",
    "publish-minor": "npm run preversion && npm version minor && git push origin master --tags && npm publish",
    "publish-major": "npm run preversion && npm version major && git push origin master --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MRN-Code/nodeapi.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MRN-Code/nodeapi/issues"
  },
  "homepage": "https://github.com/MRN-Code/nodeapi",
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}
